{
    "name": "Stored Links",
    "version": "0.6.1",
    "description": "Save your links into a list",
    "manifest_version": 2,
    "author": "Felix Adamietz aka VolleInspiration",
    "icons": 
    {
      "128": "assets/images/icon128.png"
    },
    "background":
    {
      "scripts": ["assets/js/blobDownload.js"]
    },
    "browser_action": 
    {
        "default_popup": "popup.html",
        "browser_style": true
    },
    "permissions":[
        "storage",
        "downloads",
        "activeTab",
        "webRequest"
    ],
    "browser_specific_settings": 
    {
      "gecko": 
      {
        "id": "StorageLinks@VolleInspiration",
        "strict_min_version": "100.0"
      }
    },
    "incognito": "not_allowed"

  }